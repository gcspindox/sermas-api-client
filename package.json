{
  "name": "@sermas/api-client",
  "version": "1.0.0",
  "description": "Sermas API client",
  "main": "dist/index.js",
  "scripts": {
    "dev": "NODE_NO_WARNINGS=1 NODE_TLS_REJECT_UNAUTHORIZED=0 nodemon -w ./src/index.ts",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc --build tsconfig.json",
    "build:watch": "npm run clean && tsc -w",
    "test": "jest",
    "generate:openapi": "rm -rf ./src/libs/openapi && openapi --input ./openapi.json --output ./src/libs/openapi --name SermasApi --client axios",
    "generate:asyncapi": "rm -rf ./src/libs/asyncapi && mkdir -p ./src/libs/asyncapi && ts-node ./src/asyncapi-generator.ts ./src/asyncapi-generator.ts",
    "generate:asyncapi:dev": "nodemon --watch ./src/asyncapi-generator.ts ./src/asyncapi-generator.ts"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "form-data": "^4.0.0",
    "glob": "^10.3.10",
    "js-yaml": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "mqtt": "^5.3.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@asyncapi/modelina": "^3.1.0",
    "@asyncapi/parser": "^3.0.5",
    "@types/js-yaml": "^4.0.9",
    "@types/mqtt": "^2.5.0",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "dotenv": "^16.4.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "openapi-typescript-codegen": "^0.26.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "author": "",
  "license": "Apache-2.0"
}